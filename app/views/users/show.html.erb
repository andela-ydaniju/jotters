Welcome <%= @user.name %>

<%= link_to "Log out", session_path, method: :delete%>

<% @jotters.each do |jotter| %>
<div>
<%= jotter.title %>: <%= jotter.body %>
<%= link_to "Edit jotter", edit_jotter_path(jotter.id) %>
<%= link_to "Delete", jotter, method: :delete, data:
      { confirm: "Do you want to delete this jotter?" } %> 
</div>
<%end%>

<%= link_to "Create New Jotter", new_jotter_path %>